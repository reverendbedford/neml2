<materials>
  <isolinear type="GeneralIntegrator">
    <elastic type="IsotropicLinearElasticModel">
      <m1>124000.0</m1>
      <m1_type>youngs</m1_type>
      <m2>0.32</m2>
      <m2_type>poissons</m2_type>
    </elastic>

    <rule type="TVPFlowRule">
      <elastic type="IsotropicLinearElasticModel">
        <m1>124000.0</m1>
        <m1_type>youngs</m1_type>
        <m2>0.32</m2>
        <m2_type>poissons</m2_type>
      </elastic>

      <flow type="PerzynaFlowRule">
        <surface type="IsoJ2"/>
        <hardening type="LinearIsotropicHardeningRule">
          <s0>10.0</s0>
          <K>5500.0</K>
        </hardening>
        <g type="GPowerLaw">
          <n>5.0</n>
          <eta>500.0</eta>
        </g>
      </flow>
    </rule>
  </isolinear>
  <combined type="GeneralIntegrator">
    <elastic type="IsotropicLinearElasticModel">
      <m1>124000.0</m1>
      <m1_type>youngs</m1_type>
      <m2>0.32</m2>
      <m2_type>poissons</m2_type>
    </elastic>

    <rule type="TVPFlowRule">
      <elastic type="IsotropicLinearElasticModel">
        <m1>124000.0</m1>
        <m1_type>youngs</m1_type>
        <m2>0.32</m2>
        <m2_type>poissons</m2_type>
      </elastic>

      <flow type="PerzynaFlowRule">
        <surface type="IsoKinJ2"/>
      <hardening type="CombinedHardeningRule">
        <iso type="LinearIsotropicHardeningRule">
          <s0>10.0</s0>
          <K>2500.0</K>
        </iso>
        <kin type="LinearKinematicHardeningRule">
          <H>1000.0</H>
        </kin>
      </hardening>
        <g type="GPowerLaw">
          <n>5.0</n>
          <eta>500.0</eta>
        </g>
      </flow>
    </rule>
  </combined>
</materials>
